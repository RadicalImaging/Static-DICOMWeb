  "engines": {
    "node": ">=14.18.1",
    "npm": ">=6.14.15",
    "yarn": ">=1.22.4"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/OHIF/static-wado",
    "directory": "packages/static-wado-deploy"
  },
  "bugs": {
    "url": "https://github.com/OHIF/static-wado/issues"
