{
  "name": "@radicalimaging/static-wado-webserver",
  "version": "0.6.5",
  "description": "Webserver to serve a static-wado repo",
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "StaticWado",
    "StaticWadoServer",
    "DICOM",
    "DICOMweb"
  ],
  "author": "Bill Wallace <bill.wallace@radicalimaging.com>",
  "contributors": [
    "Chris Hafey <chafey@gmail.com>",
    "Rodolfo Costa <rodolfo.costa@radicalimaging.com>"
  ],
  "engines": {
    "node": ">=14.18.1",
    "npm": ">=6.14.15",
    "yarn": ">=1.22.4"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/OHIF/static-wado",
    "directory": "packages/static-wado-scp"
  },
  "bugs": {
    "url": "https://github.com/OHIF/static-wado/issues"
  },
  "homepage": "https://github.com/OHIF/static-wado#readme",
  "license": "ISC",
  "main": "dist/index.js",
  "module": "lib/index.ts",
  "directories": {
    "lib": "lib",
    "bin": "bin"
  },
  "files": [
    "bin",
    "lib"
  ],
  "bin": {
    "dicomwebserver": "bin/dicomwebserver.mjs"
  },
  "scripts": {
    "clean": "rimraf dist",
    "dicomwebserver": "yarn build && node bin\\dicomwebserver.mjs",
    "dev": "cross-env NODE_ENV=development webpack --config .webpack/webpack.dev.js --watch --debug --output-pathinfo",
    "dev:dicom-pdf": "yarn run dev",
    "link:exec": "npm link",
    "build": "cross-env NODE_ENV=production webpack --config .webpack/webpack.prod.js",
    "test": "jest --config ./jest.config.js",
    "build:package": "yarn run build",
    "start": "yarn run dev",
    "lint": "npx eslint --color \"*(bin|lib)/**/*.{js,mjs}\"",
    "lint:fix": "npx eslint --fix --color \"*(bin|lib)/**/*.{js,mjs}\""
  },
  "dependencies": {
    "@radicalimaging/static-wado-plugins": "^0.6.2",
    "@radicalimaging/static-wado-util": "^0.6.2",
    "config-point": "^0.5.0",
    "cors": "^2.8.5",
    "dcmjs": "^0.19.6",
    "dcmjs-dimse": "^0.0.15",
    "express": "^4.17.2",
    "formidable": "^2.0.1",
    "morgan": "^1.10.0",
    "regenerator-runtime": "^0.13.9",
    "rxjs": "^7.5.4"
  },
  "devDependencies": {
    "@babel/plugin-transform-runtime": "^7.17.0",
    "must": "^0.13.4"
  }
}
