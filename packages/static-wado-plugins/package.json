{
  "name": "@radicalimaging/static-wado-plugins",
  "version": "0.6.3",
  "description": "Plugins to support the static-wado project",
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "StaticWado",
    "DICOM",
    "DICOMweb"
  ],
  "author": "Bill Wallace <bill.wallace@radicalimaging.com>",
  "collaborators": [
    "Rodolfo Costa <rodolfo.costa@radicalimaging.com>"
  ],
  "engines": {
    "node": ">=14.18.1",
    "npm": ">=6.14.15",
    "yarn": ">=1.22.4"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/OHIF/static-wado",
    "directory": "packages/static-wado-plugins"
  },
  "bugs": {
    "url": "https://github.com/OHIF/static-wado/issues"
  },
  "homepage": "https://github.com/OHIF/static-wado#readme",
  "license": "ISC",
  "main": "dist/index.js",
  "module": "lib/index.js",
  "directories": {
    "lib": "lib"
  },
  "files": [
    "lib"
  ],
  "scripts": {
    "clean": "rimraf dist",
    "dev": "cross-env NODE_ENV=development webpack --config .webpack/webpack.dev.js --watch --debug --output-pathinfo",
    "link:exec": "npm link",
    "build": "cross-env NODE_ENV=development webpack --config .webpack/webpack.prod.js",
    "test": "jest --config ./jest.config.js",
    "build:package": "yarn run build",
    "start": "yarn run dev",
    "lint": "npx eslint --color \"*(bin|lib)/**/*.{js,mjs}\"",
    "lint:fix": "npx eslint --fix --color \"*(bin|lib)/**/*.{js,mjs}\""
  },
  "jest": {
    "verbose": true
  },
  "dependencies": {
    "@radicalimaging/static-wado-util": "^0.6.2",
    "config-point": "^0.4.6",
    "dcmjs-dimse": "^0.0.15",
    "http-proxy-middleware": "^2.0.6"
  },
  "devDependencies": {
    "must": "^0.13.4"
  }
}
