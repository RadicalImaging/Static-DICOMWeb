{
  "name": "@radicalimaging/dicomweb-worker",
  "version": "1.5.0",
  "description": "Provides various webworker builds with command line testing tools to use as utilities",
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "StaticWado",
    "DICOM",
    "DICOMweb"
  ],
  "author": "Bill Wallace <bill.wallace@radicalimaging.com>",
  "contributors": [
    "Rodolfo Costa <rodolfo.costa@radicalimaging.com>"
  ],
  "bugs": {
    "url": "https://github.com/radicalimaging/static-dicomweb/issues"
  },
  "homepage": "https://github.com/radicalimaging/static-dicomweb#readme",
  "license": "ISC",
  "main": "lib/index.js",
  "module": "dist/index.js",
  "directories": {
    "lib": "lib",
    "dist": "dist",
    "bin": "bin"
  },
  "files": [
    "bin",
    "dist",
    "lib"
  ],
  "bin": {
    "mkdicomweb": "bin/mkdicomweb.mjs"
  },
  "scripts": {
    "test": "jest --config ./jest.config.js",
    "build": "bun run build:esm",
    "build:esm": "bun build lib/index.mjs --target node --outdir dist --",
    "clean": "rimraf dist",
    "link:exec": "npm install -g && npm link",
    "lint": "npx eslint --color \"**/*.{js,mjs,cjs}\"",
    "pack:js": "npm pack --",
    "lint:fix": "npx eslint --fix --color \"**/*.{js,mjs,cjs}\""
  },
  "jest": {
    "verbose": true
  },
  "dependencies": {
    "@cornerstonejs/dicom-codec": ">=1.0.5",
    "@radicalimaging/static-wado-util": "^1.5.0",
    "canvas": "^2.11.1",
    "config-point": ">=0.5.1",
    "dcmjs": "0.35.0",
    "dicom-parser": "^1.8.13",
    "node-object-hash": "^3.0.0",
    "uuid": "^11.0.2",
    "glob": "^11.0.0"
  },
  "devDependencies": {
    "must": "^0.13.4"
  }
}
